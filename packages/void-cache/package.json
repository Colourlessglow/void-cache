{
  "name": "void-cache",
  "version": "0.1.1",
  "description": "一个为了统一开发中各种key-value储存方案API的小工具",
  "repository": "Colourlessglow/void-cache",
  "scripts": {
    "build": "unbuild"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": {
    "name": "whitekite",
    "email": "xuxjigsaw@qq.com"
  },
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@void-cache/core": "workspace:*",
    "@void-cache/storage-like": "workspace:*",
    "@void-cache/web-storage": "workspace:*",
    "@void-cache/memory": "workspace:*",
    "@void-cache/indexdb": "workspace:*"
  }
}
